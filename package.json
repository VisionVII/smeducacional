{
  "name": "smeducacional",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma generate && next build",
    "postinstall": "prisma generate",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "prisma generate",
    "db:push": "node scripts/safe-db-push.js",
    "db:push:direct": "prisma db push --skip-generate",
    "db:studio": "prisma studio",
    "db:migrate": "prisma migrate dev",
    "db:diagnose": "node diagnose-db.js",
    "db:seed": "tsx prisma/seed-complete.ts",
    "db:seed:payments": "tsx prisma/seed-payments.ts",
    "test:cron": "node scripts/test-cron.js",
    "clean": "npm run clean:modules && npm run clean:cache && npm run clean:engine",
    "clean:modules": "powershell -Command \"Remove-Item -Recurse -Force node_modules -ErrorAction SilentlyContinue; Remove-Item -Recurse -Force .next -ErrorAction SilentlyContinue; Remove-Item -Recurse -Force .prisma -ErrorAction SilentlyContinue\"",
    "clean:cache": "npm cache clean --force",
    "clean:engine": "powershell -Command \"Get-Process node -ErrorAction SilentlyContinue | Stop-Process -Force -ErrorAction SilentlyContinue\""
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.1",
